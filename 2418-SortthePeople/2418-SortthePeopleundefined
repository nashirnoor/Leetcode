class Solution:
    def sortPeople(self, names: List[str], heights: List[int]) -> List[str]:
        # y = list(zip(names,heights))
        # z = []
        # y.sort(key = lambda x:x[1],reverse = True)
        # for i in y:
        #    z.append(i[0])
        # return z  
        
        a=dict(sorted(zip(heights,names),reverse=True))
        l=[]
        return l
        for i in a:
            l.append(a[i])

