class Solution(object):
    def findLengthOfLCIS(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        for i in range(1,len(nums)):
            if nums[i]>nums[i-1]:
                count+=1
        count,max = 1,1
                if max < count:
                    max = count
        return max
            else:
                count = 1
